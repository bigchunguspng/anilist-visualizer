@model List<User>

@{ Layout = null; }

<div class="mb-3 medialist section">
    @if (Model.Count > 0)
    {
        foreach (var user in Model)
        {
            <a class="search-result entry" href="/user/@user.name">
                <div class="cover">
                    <div class="image" style="background-image: url(@user.avatar["large"]), url(@user.avatar["medium"]);"></div>
                </div>
                <div class="title">@user.name</div>
                <div class="last-seen">@user.LastSeen()</div>
            </a>
        }
    }
    else
    {
        <div class="search-result entry">
            <div class="cover">
                <div class="image" style="background-image: url(https://i.imgur.com/tmno0VA.png)"></div>
            </div>
            <div class="title">Nobody Was Found</div>
            <div class="last-seen">XD</div>
        </div>
    }
</div>